    "buffer": "^6.0.3",
    "fs-extra": "^10.1.0",
    "nodemon": "^2.0.19",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "dependencies": {
    "nanoid": "^4.0.0",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "spred": "^0.31.1",
    "spred-dom": "^0.7.3"
  },
